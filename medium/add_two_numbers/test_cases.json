{
    "func": "addTwoNumbers",
    "tests": {
        "1": {
            "input": {
                "l1": [2,4,3],
                "l2": [5,6,4]
            },
            "expected": [7,0,8]
        },
        "2": {
            "input": {
                "l1": [0],
                "l2": [0]
            },
            "expected": [0]
        },
        "3": {
            "input": {
                "l1": [9,9,9,9,9,9,9],
                "l2": [9,9,9,9]
            },
            "expected": [8,9,9,9,0,0,0,1]
        }
    }
}